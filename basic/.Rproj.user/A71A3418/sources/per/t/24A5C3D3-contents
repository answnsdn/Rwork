#R에서 사용하는 주석문
num = 100 #num이 100이다.
num
num <- 1000 #num변수에 1000을 저장하라
num
num==1000 #num이 1000과 같은지 비교 (TRUE/FALSE)
num==100
num!=100
num>100
num<100
test <- "myR"
test
myR <- num
test<-myR
test


result1 <- 100 #숫자형
result1
result2 <- "test" #문자형
result2


#R에서 제공되는 함수를 이용할 수 있다.
#Class함수 : 타입을 확인
class(result1) #숫자 : numeric
class(result2) #글자 : character
#as.---함수 : 원하는 ---타입으로 바꿔준다.
test <- as.character(result1)
test
class(test)
test <- as.numeric(result1)
test
